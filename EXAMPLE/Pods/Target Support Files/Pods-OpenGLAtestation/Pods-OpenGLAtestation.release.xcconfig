GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/HRBaseShader" "${PODS_ROOT}/Headers/Public/HRSubclasses"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/HRBaseShader" "$PODS_CONFIGURATION_BUILD_DIR/HRSubclasses"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/HRBaseShader" -isystem "${PODS_ROOT}/Headers/Public/HRSubclasses"
OTHER_LDFLAGS = $(inherited) -ObjC -l"HRBaseShader" -l"HRSubclasses"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
